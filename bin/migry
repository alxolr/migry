#!/usr/bin/env node

const program = require('commander');

program
  .version('0.0.2')
  .option('-c, --create [name]', 'Create a new migration <name>', '')
  .option('-l, --list', 'List existing migrations')
  .option('-u, --up [name]', 'Migrate up <name>', '')
  .option('-d, --down [name]', 'Migrate down <name>', '')
  .parse(process.argv);
